separator=false
separator_block_width=18

[network]
command= ~/.config/i3blocks/network
interval=10

[fcitx5]
command= ~/.config/i3blocks/fctix5
interval=-1
signal=4

[brightness]
command= ~/.config/i3blocks/bright
interval=-1
signal=3

[microphone]
command= ~/.config/i3blocks/microphone
interval=-1
signal=2

[volume]
command= ~/.config/i3blocks/volume
interval=-1
signal=1

[battery]
command= ~/.config/i3blocks/battery
interval=10

[time]
command=date "+%a %b %d %H:%M"
interval=5
